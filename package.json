{
  "name": "@dr-kobros/react-broilerplate-project",
  "version": "1.2.0",
  "description": "An example project utilizing broilerplate",
  "main": "src/client.jsx",
  "scripts": {
    "clean": "rimraf ./dist",
    "build:webpack": "NODE_ENV=prod webpack",
    "build": "npm run clean && npm run build:webpack",
    "lint": "eslint src --ext js --ext jsx",
    "babel-node": "babel-node",
    "start": "NODE_ENV=development nodemon --exec npm run babel-node -w src/server.js -- src/server.js",
    "test": "ava -v"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pekkis/react-broilerplate-project"
  },
  "keywords": [
    "team"
  ],
  "author": "Pekkis",
  "license": "BSD-3-Clause",
  "dependencies": {
    "@dr-kobros/react-broilerplate": "^0.11.0",
    "@dr-kobros/react-broilerplate-server-express": "^0.4.0",
    "axios": "^0.12.0",
    "body-parser": "^1.15.1",
    "classnames": "^2.2.5",
    "eslint-plugin-import": "^1.8.0",
    "eslint-plugin-jsx-a11y": "^1.2.0",
    "immutable": "^3.8.1",
    "node-uuid": "^1.4.7",
    "normalize.css": "^4.1.1",
    "react-fa": "^4.1.1",
    "react-immutable-proptypes": "^1.7.1",
    "react-sound": "^0.5.0"
  },
  "devDependencies": {
    "eslint": "^2.9.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-react": "^5.1.1"
  }
}
